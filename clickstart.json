{
    "id": "spring-petclinic-loaderio-clickstart",
    "name": "Spring PetClinic App for loader.io",
    "source": "github",
    "description" : "Spring PetClinic App deployed on Tomcat7 via Jenkins with maven",
    "doc-url" : "https://github.com/CloudBees-community/spring-petclinic-clickstart/blob/master/readme.md",
    "icon" : "https://raw.github.com/CloudBees-community/spring-petclinic-clickstart/master/clickstart-icon.png",

    "disabled" : false,
    "supported-by" : "community",
    "app-package" : "https://community.ci.cloudbees.com/job/spring-petclinic-clickstart-loader-io/lastSuccessfulBuild/artifact/target/spring-petclinic-loaderio-1.0.0-SNAPSHOT.war",
    "order" : 3,
   
    "runtime": {
        "type": "tomcat7",
        "app-variables": {
            "spring.profiles.active": "default,javaee",
            "jdbc.initLocation" :"classpath:db/mysql/initDB.sql",
            "jdbc.dataLocation": "classpath:db/mysql/populateDB.sql",
            "hibernate.dialect": "org.hibernate.dialect.MySQLDialect",
            "jpa.database":"MYSQL"
        }
    },
    "databases": [
        {
            "type": "mysql",
            "alias": "petclinic"
        }
    ]
}
